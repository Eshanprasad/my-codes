n=int(input('enter the number: '))
if n==0 or n==1:
    print('Not prime number')
else:
    for i in range(2,n):
        if n%i==0:
            print('Not prime number')
            break
    else:
        print('Prime number')




# While loop
n = int(input('enter any number:'))
d = 2
flag = False
while d < n:
    if n % d == 0:
        flag = True
    d = d+1

if flag == True:
    print('Not prime number')
else:
    print('prime')
